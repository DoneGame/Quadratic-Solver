all: quad.exe


quad.exe: main.o input.o solver.o output.o testing.o agrs_handler.o color.o
	g++ main.o input.o solver.o output.o testing.o agrs_handler.o color.o -o quad.exe

main.o: main.cpp
	g++ -c main.cpp

input.o: input.cpp
	g++ -c input.cpp

solver.o: solver.cpp
	g++ -c solver.cpp

output.o: output.cpp
	g++ -c output.cpp

testing.o: testing.cpp
	g++ -c testing.cpp

agrs_handler.o: agrs_handler.cpp
	g++ -c agrs_handler.cpp

color.o: color.cpp
	g++ -c color.cpp


clean:
	rm -rf *.o quad.exe
